{
  "name": "vite-vue-ts-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-ts-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-vue-ts-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "add:ios": "npm run build && rimraf ios/ && npx cap add ios",
    "open:ios": "npx cap open ios",
    "run:ios": "npx cap run ios",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}": [
      "npx prettier --write --loglevel warn "
    ],
    "src/**/*.{vue,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@capacitor/core": "^3.6.0",
    "@capacitor/ios": "^3.6.0",
    "@vueuse/core": "^8.9.2",
    "aplayer": "^1.10.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.6.0",
    "@types/node": "^17.0.45",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-windicss": "^1.8.6",
    "windicss": "^3.5.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.6.0"
  }
}
